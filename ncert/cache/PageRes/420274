<!DOCTYPE html><html><head><meta http-equiv="content-type"  content="text/html; charset=utf-8" /><meta http-equiv="Cache-Control" content="no-cache" /><meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0, user-scalable=no" /><meta name="MobileOptimized"/><title>Error!</title><style>body {margin: 0;padding: 0;font: 15px/22px arial,Gotham-Medium,"\5FAE\8F6F\96C5\9ED1";background:#fff; color: #6f8aad;}p {margin: 0;}img {border: 0;vertical-align: middle;}.msg{	padding: 10px 0;}.error-img0{	background-image:url(file:///android_asset/UCMobile/help/images/error.png);	background-repeat: no-repeat;	height: 227px;}.error-img0{	background-position: 50% 0;	background-size: 219px 227px;}.error-txt {	margin: 34px 10px 0;	font-size: 20px;	line-height: 24px;	color: #525252;	text-align:center;}.error-txt {	margin: 32px 10px 0;	font-size: 20px;	line-height: 24px;	color: #525252;	text-align:center;}.links{	width:300px;	margin: 33px auto 0;}.ctrl-bar {	font-size: 0;	text-align:center;}.ctrl-bar a{	display: inline-block;	width: 135px;	line-height: 39px;	height: 39px;	font-size: 16px;	border: 1px solid #ddd;	border-radius: 3px;	margin: 0 5px;	text-decoration: none;}.ctrl-bar .icon-refresh{	color: #666;}.ctrl-bar .icon-back{	color: #52ade7;}.error-details {	text-align:right;	margin: 15px 8px 0;}.error-details a{	color: #52ade7;	text-decoration: none;}</style></head><body>	<div class="msg" id="msg">		<div class="error-img0" id="error_img"></div>		<div class="error-txt" id="error_txt">Oops! Connection failed.</div>		<div class="links">			<div class="ctrl-bar"> <a class="icon-refresh" href="ext:refresh">Refresh</a> <a class="icon-back" href="ext:back">Back</a> </div>			<div class="error-details"><a class="error-redirect" href="ext:lp:lp_netErrorInfo:WifiState=true;DefProxy=null;APN=unknown;UsedProxy=null;NetConnection=1; ;Url=http://www.google.co.in:80/m;IP=www.google.co.in/216.58.197.67:80;IPNum=1;UseFoxyServer=0; ;FailReason=OpenConnection;ErrorCode=-8;">Error details</a></div>			<div class="error-details" style="margin-top:0;"><a class="error-redirect" href="ext:feedback">Contact Miss UC</a></div>		</div>	</div></body><script type="text/javascript">//状态码赋值var error_num = -8;//所有参数及对照信息修改处var error_state = {	"-80":["error-img0","You are not connected to the Internet."],	"-2":["error-img0","Oops! This link appears to be expired."],	"-6":["error-img0","Cannot find the server."],	"-9":["error-img0","Server error. Press Refresh or go to another page."],	"-8":["error-img0","Request time-out. Press Refresh to try again."],	"-11":["error-img0","Oops! Connection Failed."],	"-4x":["error-img0","Server error. Press Refresh or go to another page."],	"msg_num":["error-img0","Oops! Connection failed."]}//获取需要修改的页面无素[图片+文字]var error_img = document.getElementById("error_img");var error_txt = document.getElementById("error_txt");//区分三种状态码，执行对应函数function msg(){	if(error_num.toString().match(/^\d{3}$/ig)){		msg_num(error_num);	}else if(error_num.toString().match(/^-[4]\d{1}$/ig)){		msg_state("-4x");	}else if(error_num.toString().match(/^-\d{1,2}$/ig)){		msg_state(error_num);	}}function msg_num(num){	error_img.className = error_state.msg_num[0];	error_txt.innerHTML = error_state.msg_num[1]+" - "+num;}function msg_state(n){	error_img.className = error_state[n][0];	error_txt.innerHTML = error_state[n][1];}//自动居中function auto_mid(){	var msg = document.getElementById("msg");	var bodyH = document.body.clientHeight;	var screenH = document.documentElement.clientHeight;	if(bodyH<screenH){		msg.style.paddingTop = ((screenH-bodyH)/2)+10+"px";	}}msg();auto_mid();</script></html>